package com.mycompany.bai2chap3;

/**
 *
 * @author Admin
 */
public class NumberIn extends Thread {
    public NumberIn(String name){
        super(name);
    }
    
    @Override
    public void run(){
        for(int i=1;i<=10;i++){
            System.out.println(getName()+":"+i);
        }
    }
}
